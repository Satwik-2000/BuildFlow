{
    "buildCommand": "npm install && npm run build:full",
    "outputDirectory": "frontend/dist",
    "env": {
        "DATABASE_URL": "@database_url"
    },
    "rewrites": [
        {
            "source": "/graphql",
            "destination": "/api"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ]
}